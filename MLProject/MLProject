name: Diabetes_Prediction_MLProject

conda_env: conda.yaml

entry_points:
  main:
    parameters:
      test_size: {type: float, default: 0.2}
      random_state: {type: int, default: 42}
      n_estimators: {type: int, default: 100}
      max_depth: {type: int, default: 10}
    command: "python modelling.py --test-size {test_size} --random-state {random_state} --n-estimators {n_estimators} --max-depth {max_depth}"